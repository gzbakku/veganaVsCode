{
	"publisher": "tejasavdutt",
	"name": "veganavscode",
	"displayName": "veganaVsCode",
	"description": "autocompletion for vegana js",
	"version": "0.0.3",
	"license" : "SEE LICENSE IN LICENSE.md",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Extension Packs","Snippets"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onCommand:vegana"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./javascript_snippets.json"
			}
		],
		"languages": [
			{
				"id": "javascript",
				"extensions": [
					".js"
				]
			}
		],
		"commands": [
			{
				"command": "vegana",
				"title": "Vegana JS"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
